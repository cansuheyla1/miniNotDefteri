
<!DOCTYPE html>
<html lang="en">
<%- include("./not_partials/not_head.ejs") %>
<body>
    <%- include("./not_partials/not_nav.ejs") %>

   <div class="all-notes content">
        <h2>All Notes</h2>
        <% if (notes.length>0){ %>
            <% notes.forEach(note => { %>
                <h3 class="title"><%= note.title %></h3>
                <p class="content"><%= note.content %></p>
            <% }) %>
        <% } else { %>
            <p>There are no notes to display</p>
        <% } %>
    </div>

    <%- include("./not_partials/not_footer.ejs") %>
</body>
</html>